{
    "name": "playground/flow",
    "description": "getting and sending flow",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "zf2",
        "playground",
        "flow"
    ],
    "homepage": "https://github.com/gregorybesson/PlaygroundFlow",
    "authors": [
        {
            "name": "Greg Besson",
            "email": "gregory.besson@playground.gg",
            "homepage": "http://www.playground.gg"
        }
    ],
    "repositories" : [{
			"type" : "git",
			"url" : "https://github.com/gregorybesson/hybridauth"
		}
	],
    "require": {
        "php" : ">=5.5",
        "zendframework/zend-developer-tools" : ">=1.0.0",
		"zendframework/zendframework" : ">=2.0.0",
		"zf-commons/zfc-base" : ">=0.0.0",
		"playground/core" : ">=2.0.0",
		"playground/design" : ">=2.0.0",
		"playground/user" : ">=2.0.0",
		"playground/reward" : ">=2.0.0"
    },
    "require-dev": {
        "doctrine/doctrine-orm-module": ">=0.0.0",
        "phpunit/phpunit" : ">=4.0.0",
        "squizlabs/php_codesniffer": ">=2.0.0",
        "php-coveralls/php-coveralls": ">=0.0.0"
    },
    "autoload" : {
		"psr-0" : {
			"PlaygroundFlow" : "src/"
		}
	},
	"extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    }
}

{
    "name": "playground/flow",
    "description": "getting and sending flow",
    "type": "library",
    "license": "BSD-3-Clause",
    "keywords": [
        "zf2",
        "playground",
        "flow"
    ],
    "homepage": "https://github.com/gregorybesson/PlaygroundFlow",
    "authors": [
        {
            "name": "Greg Besson",
            "email": "gregory.besson@playground.gg",
            "homepage": "http://www.playground.gg"
        }
    ],
    "repositories" : [{
			"type" : "git",
			"url" : "https://github.com/gregorybesson/hybridauth"
		}
	],
    "require": {
        "php" : ">=5.5",
        "zendframework/zend-developer-tools" : "1.*@RC",
		"zendframework/zendframework" : "2.*",
		"zf-commons/zfc-base" : "0.*",
		"playground/core" : "2.*",
		"playground/design" : "2.*",
		"playground/user" : "2.*",
		"playground/reward" : "2.*"
    },
    "require-dev": {
        "doctrine/doctrine-orm-module": "0.*",
        "phpunit/phpunit" : "4.*",
        "squizlabs/php_codesniffer": "2.*",
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload" : {
		"psr-0" : {
			"PlaygroundFlow" : "src/"
		}
	},
	"extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    }
}
